<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="webscraping.coursework.Url" table="url">
    <id name="id" type="int">
      <column name="id"/>
      <generator class="increment"/>
    </id>
    <property name="domain" type="string">
      <column name="domain"/>
    </property>
    <property name="path" type="string">
      <column name="path"/>
    </property>
    <property name="queryString" type="string">
      <column name="query_string"/>
    </property>
  </class>
</hibernate-mapping>
